console.log('js연결확인');

// 사용자가 데이터 불러오기 버튼을 클릭했을 때 

$('#btn').on('click', request);


function request(){
	
	// 요청을 보내고 JSON 형식으로 데이터 응답 받기
	$.ajax({
		url: 'ajax02',
		type:'get',
		data:{
			
		},
		success : function(res){
			console.log(res);
		},
		error : function(e){
			console.log("error")	
		}
		
		
	}) 
	
	
	
	
	
	
}